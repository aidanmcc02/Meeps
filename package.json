{
  "name": "meeps",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:web": "node scripts/write-env-config.js && cross-env BASE_URL=/ vite build",
    "build:check": "npm run build:web",
    "preview": "vite preview",
    "start": "serve -s dist",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "generate-icon": "node scripts/generate-icon.js",
    "generate-latest-json": "node scripts/generate-latest-json.js",
    "generate-pwa-icons": "node scripts/generate-pwa-icons.js",
    "update-icons": "node scripts/update-icons.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint --fix --ext .js,.jsx .",
    "lint:check": "eslint --ext .js,.jsx .",
    "lint-staged": "lint-staged",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "overrides": {
    "minimatch": "10.2.4",
    "serialize-javascript": "7.0.3",
    "serve": {
      "ajv": "8.18.0"
    },
    "eslint": {
      "ajv": "6.12.6"
    }
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@tauri-apps/api": "^1.0.0",
    "eslint": "^10.0.2",
    "globals": "^17.4.0",
    "husky": "^9.1.7",
    "prettier": "^3.8.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-markdown": "^9.0.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.0",
    "cross-env": "^10.1.0",
    "lint-staged": "^16.3.0",
    "postcss": "^8.4.0",
    "serve": "^14.2.5",
    "tailwindcss": "^3.4.0",
    "vite": "^7.3.1",
    "vite-plugin-pwa": "^1.2.0"
  }
}
